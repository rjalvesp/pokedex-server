{
  "settings": {
    "propWrapperFunctions": ["forbidExtraProps"]
  },
  "rules": {
    "array-bracket-spacing": ["error", "never"],
    "arrow-spacing": "error",
    "block-spacing": "error",
    "brace-style": "error",
    "comma-spacing": ["error", { "before": false, "after": true }],
    "curly": "error",
    "default-case": "error",
    "eqeqeq": "error",
    "eol-last": ["error", "always"],
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "key-spacing": ["error", { "beforeColon": false }],
    "keyword-spacing": "error",
    "max-depth": ["error", 4],
    "max-lines-per-function": ["error", { "max": 30, "skipBlankLines": true }],
    "no-const-assign": "error",
    "no-console": 1,
    "no-dupe-keys": "error",
    "no-duplicate-case": "error",
    "no-duplicate-imports": "error",
    "no-dupe-args": "error",
    "no-dupe-else-if": "error",
    "no-empty": "error",
    "no-empty-function": "error",
    "no-eval": "error",
    "no-irregular-whitespace": "error",
    "no-return-await": "error",
    "no-trailing-spaces": ["error", { "ignoreComments": true }],
    "no-unreachable": "error",
    "no-unreachable-loop": "error",
    "no-unused-expressions": "error",
    "no-unused-vars": "error",
    "no-useless-escape": 0,
    "no-useless-catch": "error",
    "no-var": "error",
    "no-whitespace-before-property": "error",
    "object-curly-spacing": [
      "error",
      "always",
      { "arraysInObjects": true, "objectsInObjects": true }
    ],
    "prefer-const": "error",
    "quotes": ["error", "double"],
    "ramda/always-simplification": "error",
    "ramda/any-pass-simplification": "error",
    "ramda/both-simplification": "error",
    "ramda/complement-simplification": "error",
    "ramda/compose-pipe-style": "off",
    "ramda/compose-simplification": "error",
    "ramda/cond-simplification": "error",
    "ramda/either-simplification": "error",
    "ramda/eq-by-simplification": "error",
    "ramda/filter-simplification": "error",
    "ramda/if-else-simplification": "error",
    "ramda/map-simplification": "error",
    "ramda/merge-simplification": "error",
    "ramda/no-redundant-and": "error",
    "ramda/no-redundant-not": "error",
    "ramda/no-redundant-or": "error",
    "ramda/pipe-simplification": "error",
    "ramda/prefer-both-either": "error",
    "ramda/prefer-complement": "error",
    "ramda/prefer-ramda-boolean": "error",
    "ramda/prop-satisfies-simplification": "error",
    "ramda/reduce-simplification": "error",
    "ramda/reject-simplification": "error",
    "ramda/set-simplification": "error",
    "ramda/unless-simplification": "error",
    "ramda/when-simplification": "error",
    "rest-spread-spacing": ["error", "never"],
    "semi": "error",
    "semi-spacing": ["error", { "before": false, "after": true }],
    "space-before-blocks": "error",
    "yoda": "error"
  },
  "plugins": ["ramda"],
  "env": {
    "es6": true,
    "browser": false,
    "jasmine": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "process": true,
    "global": true,
    "jasmine": true,
    "describe": true,
    "it": true,
    "test": true,
    "afterEach": true,
    "expect": true,
    "module": true,
    "require": true,
    "__dirname": true,
    "setTimeout": true,
    "console": true,
    "exports": true,
    "Buffer": true,
    "Intl": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ]
}
